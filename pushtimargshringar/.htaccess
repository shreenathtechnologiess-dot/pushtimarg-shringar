RewriteEngine On
RewriteBase /pushtimargshringar/

# Agar file ya folder exist karta hai to direct serve karo
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# Pretty URL: /product/slug â†’ product.php?slug=slug
RewriteRule ^product/([^/]+)/?$ product.php?slug=$1 [L,QSA]
